  </div>

  <div>
    <%= form.label :method_of_contact, style: "display: block" %>
    <%= form.text_field :method_of_contact %>
  </div>

  <div>
    <%= form.label :remarks, style: "display: block" %>
    <%= form.text_area :remarks %>
  </div>

  <div>
    <%= form.label :coordinator_id, style: "display: block" %>
    <%= form.text_field :coordinator_id %>
  </div>

  <div>
    <%= form.label :registry_id, style: "display: block" %>
    <%= form.text_field :registry_id %>
  </div>

  <div>
    <%= form.label :participant_id, style: "display: block" %>
    <%= form.text_field :participant_id %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
